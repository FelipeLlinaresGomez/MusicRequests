<resources>

    <!-- Application theme -->
    <style name="AppTheme" parent="AppTheme.Base">
			<item name="android:windowDisablePreview">true</item>
    </style>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppTheme.Base" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
        <item name="android:windowBackground">@color/white</item>
        <item name="android:statusBarColor">@color/colorPrimaryDark</item>
        <item name="android:textCursorDrawable">@null</item>
		<item name="android:forceDarkAllowed">false</item>
        <!-- color for widget theming, eg EditText. Doesn't effect ActionBar. -->
    	<item name="android:popupMenuStyle">@style/PopupMenuTheme</item>
        <item name="colorControlNormal">@android:color/white</item>
    </style>

	<style name="PopupMenuTheme" parent="@android:style/Widget.PopupMenu">
   	 	<item name="android:popupBackground">@android:color/white</item>
	</style>

    <style name="AppTheme.Login" parent="AppTheme.Base">
        <item name="android:statusBarColor">@color/colorPrimaryDark</item>
    </style>

    <style name="AppTheme.Menu" parent="AppTheme.Base">
        <item name="colorPrimary">@color/black</item>
    </style>

    <style name="AppTheme.Splash" parent="AppTheme.Base">
        <item name="android:statusBarColor">@android:color/transparent</item>
    </style>
    
    <style name="SplashTheme" parent="AppTheme.Base">
        <item name="android:windowBackground">@drawable/splash_image</item>
        <item name="android:statusBarColor">@color/colorPrimaryDark</item>
    </style>
    
    <style name="AppTheme.RouteHub" parent="AppTheme.Base">
        <item name="android:statusBarColor">@color/ic_blue_button_pressed</item>
    </style>
    
    <style name="RouteHubTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowBackground">@color/ic_blue_button_pressed</item>
        <item name="android:statusBarColor">@color/ic_blue_button_pressed</item>
    </style>

    <style name="DrawerIconTheme" parent="DrawerIconTheme.Base"/>

    <style name="DrawerIconTheme.Base" parent="Theme.AppCompat.Light.DarkActionBar" >
        <item name="drawerArrowStyle">@style/DrawerArrowStyle</item>
    </style>

    <style name="DrawerArrowStyle" parent="Widget.AppCompat.DrawerArrowToggle">
        <item name="spinBars">true</item>
        <item name="color">@android:color/white</item>
    </style>

    <style name="MyCheckboxTheme" parent="@style/Widget.AppCompat.CompoundButton.CheckBox">
        <item name="android:colorControlNormal">@color/ic_blue</item>
        <item name="android:colorControlActivated">@color/ic_blue</item>
        <item name="android:colorControlHighlight">@color/ic_blue</item>
        <item name="android:textAppearance">@style/TextAppearance.AppCompat</item>
