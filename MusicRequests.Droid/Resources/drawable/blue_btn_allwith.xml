<?xml version="1.0" encoding="UTF-8" ?>
 <selector xmlns:android="http://schemas.android.com/apk/res/android">
<item android:state_enabled="false" >
     <shape android:shape="rectangle">
         <stroke android:width="1dip" android:color="@color/ic_blue_button_disabled" />
         <solid android:color="@color/ic_blue_button_disabled"/>       
     </shape>
</item>
<item android:state_pressed="true">
     <shape android:shape="rectangle">
         <stroke android:width="1dip" android:color="@color/ic_blue_button_pressed" />
         <solid android:color="@color/ic_blue_button_pressed"/>       
     </shape>
 </item>  
<item >
    <shape android:shape="rectangle"  >
         <stroke android:width="1dip" android:color="@color/ic_btn_blue" />
         <solid android:color="@color/ic_btn_blue"/>       
     </shape>
 </item>
 </selector>